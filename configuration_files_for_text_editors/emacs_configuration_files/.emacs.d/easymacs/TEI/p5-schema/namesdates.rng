        <rng:ref name="label"/>
      </rng:optional>
      <rng:optional>
        <rng:ref name="model.dateLike"/>
      </rng:optional>
      <rng:optional>
        <rng:ref name="placeName"/>
      </rng:optional>
      <rng:zeroOrMore>
        <rng:ref name="model.pLike"/>
      </rng:zeroOrMore>
      <rng:zeroOrMore>
        <rng:choice>
          <rng:ref name="model.noteLike"/>
          <rng:ref name="model.biblLike"/>
