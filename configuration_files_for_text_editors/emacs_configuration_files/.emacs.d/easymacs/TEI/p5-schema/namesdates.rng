      <ref name="persEvent.content"/>
      <ref name="persEvent.attributes"/>
    </element>
  </define>
  <define xmlns="http://relaxng.org/ns/structure/1.0" name="persEvent.content">
    <group xmlns:rng="http://relaxng.org/ns/structure/1.0">
