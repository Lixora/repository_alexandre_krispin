      <ref name="persState.content"/>
      <ref name="persState.attributes"/>
    </element>
  </define>
  <define xmlns="http://relaxng.org/ns/structure/1.0" name="persState.content">
    <group xmlns:rng="http://relaxng.org/ns/structure/1.0">
      <rng:optional>
        <rng:ref name="label"/>
      </rng:optional>
      <rng:optional>
        <rng:ref name="model.dateLike"/>
      </rng:optional>
      <rng:zeroOrMore>
        <rng:ref name="model.pLike"/>
